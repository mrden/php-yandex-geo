{
    "name": "mrden/yandex-geo",
    "type": "library",
    "description": "PHP-library for interactions with geo-coding Yandex.Maps",
    "version": "2.0.0",
    "keywords": ["api", "yandex", "geo", "geocode", "geocoder", "maps"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Kuznetsov",
            "email": "kuznetsov2d@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Denis Kiselev",
            "email": "kiselevden911@yandex.ru",
            "role": "Contributer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Yandex\\Geo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "vimeo/psalm": "^5.22",
        "phpunit/phpunit": "^10.5"
    }
}
